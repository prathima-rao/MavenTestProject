pipeline {
  stages {
    stage("Checkout") {
       steps {
           echo "checkout from, Pipeline!"
         }
   }
   stage("Build") {
       steps {
           echo "Build , Pipeline!"
         }
   }
   stage("Test") {
       steps {
           echo "Testing from, Pipeline!"
         }
   }
   stage("Deploy") {
       steps {
           echo "Deploying from Pipeline!"
         }
   }
   
 } 
} 
